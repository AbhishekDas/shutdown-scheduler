{
    "name": "abhishekdas/shutdown-scheduler",
    "description": "A lightweight PHP library for managing shutdown events in your applications",
    "type": "library",
    "keywords": [
        "php",
        "shutdown",
        "scheduler",
        "callback",
        "cleanup",
        "resource-management",
        "shutdown-handler"
    ],
    "homepage": "https://github.com/abhishekdas/shutdown-scheduler",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Abhishek Das",
            "email": "abhishek@virtueplanet.com",
            "role": "Developer"
        }
    ],    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },    "autoload": {
        "psr-4": {
            "AbhishekDas\\ShutdownScheduler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-json": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    }
}
